drop table musicas

CREATE TABLE albuns
(
  id_album SERIAL,
  nome VARCHAR(255),
  id_banda INTEGER,
  PRIMARY KEY (id_album),
  FOREIGN KEY (id_banda)
  REFERENCES bandas(id_banda)
);

CREATE TABLE bandas
(
  id_banda SERIAL,
  nome VARCHAR(255),
  vocalista VARCHAR(255),
  PRIMARY KEY (id_banda)
);