CREATE TABLE musicas
(
  id_musica SERIAL,
  titulo VARCHAR(255),
  id_album INTEGER,
  id_banda INTEGER,
  PRIMARY KEY (id_musica),
  FOREIGN KEY (id_album)
  REFERENCES albuns(id_album),
  FOREIGN KEY (id_banda)
  REFERENCES bandas(id_banda)
);

